#include<stdio.h>
#include<string.h>
#define N 80
#define DEBUG 0
#define JUDGE(x) ((x)>=65&&(x)<=90)||((x)>=97)&&(x)<=122)
#define LONG(x) strlen(x)

int main(void){
	char a[N],b[N],d;
	for(;;){

		char b[N]={0};
		scanf("%s",a);
		int len1 =LONG(a);
		
		for(int i=0;i<len1;i++){
			

			
			if(JUDGE(a[i])
			b[i]=a[i];
			else
			break;
		}
		
		#if DEBUG
	        puts(a);
	        puts(b);
	    #endif    
		
		int len2=LONG(b);
		
		#if DEBUG
		printf("%d %d",len1,len2);
		#endif
		
		for(int i=len2-1;i>=0;i--){
			printf("%c",b[i]);
		}
		for(int i=len2;i<len1;i++){
			printf("%c",a[i]);
		}
	
		
		scanf("%c",&d);
		if(d == '\n'){
		return 0;
		}
		else
		printf(" ");
	}
}
