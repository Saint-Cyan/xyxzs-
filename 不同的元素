#include<stdio.h>
int main(void){
	int m,i,j,count=1,b[80];
	
	scanf("%d",&m);
	
	int a[m];
	
	for(i=0;i<m;i++){
		scanf("%d",&a[i]);
	}
	
	b[0]=a[0];
	
	for(i=1;i<m;i++){
		
		for(j=i;j>0;j--){
			if(a[i] == b[j-1])
				goto done;
		}
		
		b[i]=a[i];
		count++;
		
		done:
			;
	}
	printf("%d",count);
	return 0;
}
